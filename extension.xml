<extension xmlns="http://ns.adobe.com/air/extension/20.0">

    <id>com.mesmotronic.ane.googleplayservices</id>
    <versionNumber>1.0.0</versionNumber>
	
    <platforms>

        <platform name="Android-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>air-ane-googleplayservices-jar.jar</nativeLibrary> 
                <initializer>com.mesmotronic.ane.googleplayservices.GooglePlayServicesExtension</initializer> 
                <finalizer>com.mesmotronic.ane.googleplayservices.GooglePlayServicesExtension</finalizer> 
            </applicationDeployment> 
        </platform>
        
        <platform name="Android-x86"> 
            <applicationDeployment> 
                <nativeLibrary>air-ane-googleplayservices-jar.jar</nativeLibrary> 
                <initializer>com.mesmotronic.ane.googleplayservices.GooglePlayServicesExtension</initializer> 
                <finalizer>com.mesmotronic.ane.googleplayservices.GooglePlayServicesExtension</finalizer> 
            </applicationDeployment> 
        </platform>
        
        <platform name="default"> 
            <applicationDeployment/> 
        </platform>
    
    </platforms>
  
</extension>